class Solution {
    public int removeDuplicates(int[] nums) {
        int j = 1; // kyunki pehla elemnt to humesha unique hi hoga 
        for(int i = 1 ; i < nums.length ; i++){
            if(nums[i]!=nums[j-1]){
                nums[j] = nums[i];
                j++;
            }
        }
        return j;
        
    }
}
