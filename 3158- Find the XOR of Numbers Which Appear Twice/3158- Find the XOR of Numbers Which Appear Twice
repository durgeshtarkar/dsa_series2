class Solution {
    public int duplicateNumbersXOR(int[] nums) {
        int ans = 0;
        for(int i = 0 ; i < nums.length ; i++){
             int count = 0;
            for(int j = i ; j < nums.length ; j++){
                 if(nums[i]==nums[j]){
                    count++;
                 }
               
            }
             if(count==2){
                    ans^=nums[i];
                 }
            
        }
        return ans;
    }
}
